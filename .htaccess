RewriteEngine On
#RewriteCond %{HTTPS} on
#RewriteRule (.*) http://%{HTTP_HOST}%{REQUEST_URI} [R=302,L]
RewriteCond %{REQUEST_URI} !(public|css)
RewriteCond %{REQUEST_URI} !(\.css|\.js|\.png|\.jpg|\.gif|robots\.txt|\.docx|\.json|\.ico|\.map|\.bin|\.otf|\.mp3)$ [NC]
#RewriteCond %{REQUEST_URI} !^/bot/testboy-bot/?$
RewriteRule ^(.*)$ index.php?url=/$1 [QSA,L]
